{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "jsx": "preserve",
    "module": "esnext",
    "target": "esnext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "lib": [
      "dom",
      "dom.iterable",
      "es2020"
    ],
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "allowSyntheticDefaultImports": true,
    "noErrorTruncation": false,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@D3Chart": [
        "./library/d3chart/dist/index.js"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/server/pyodide/pyodide.js"  ],
  "exclude": [
    "node_modules",
    "temp",
    "library"
  ]
}